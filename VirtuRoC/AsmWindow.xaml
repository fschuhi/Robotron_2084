<Window x:Class="Robotron.AsmWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:fctb="clr-namespace:FastColoredTextBoxNS;assembly=FastColoredTextBox"
        xmlns:local="clr-namespace:Robotron"
        mc:Ignorable="d"
        Title="AsmWindow" >
    <Grid>
        <Grid Name="grid1" Background="Yellow">
            <!--WindowsFormsHost -->
            <!-- wf:MaskedTextBox x:Name="mtbDate" Mask="00/00/0000" /-->
            <!--/-->
            <WindowsFormsHost Name="wfh1" Background="Red" ChildChanged="wfh1_ChildChanged"  >
                <local:AsmForm x:Name="formAsm" TopLevel="False" />
            </WindowsFormsHost>
        </Grid>

    </Grid>
</Window>
